{
    "name": "oliverpool/config-utils",
    "description": "Utils for manipulating configurations (taylored for Laravel).",
    "keywords": [
        "config",
        "laravel"
    ],
    "license": "BSD-3",
    "authors": [
        {
            "name": "Oliverpool",
            "email": "oliverpool@hotmail.fr"
        }
    ],
    "type": "project",
    "require": {
        "php": ">=7.0",
        "illuminate/contracts": "^5.6",
        "illuminate/config": "^5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "5.2.*"
    },
    "conflict": {
        "phpunit/php-timer": ">=2"
    },
    "autoload": {
        "psr-4": {
            "Oliverpool\\Config\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Oliverpool\\Config\\Tests\\": "tests/"
        }
    }
}
